		.FPut (0; (("done"; "right")); .F
		(
			.FPut (0; (); .R (("RIGHT")));

			.FPut (0; (("done"; "left"); ("rvalue"; "right")); .F
			(
				.FPut (0; (); .R (("LEFT")))
			))
		));

		.Go (.E ("ENV"; ("this")))
	));

	.FEnv (("E"; '44.1."="); .F
	(
		.FOut (.R (("LEFT")); (
			("lvalue"; "left")));

		.FOut (.R (("RIGHT")); (
			("rvalue"; "right")));

		.Go (.E ("ENV"; ("this")))
	))
)
