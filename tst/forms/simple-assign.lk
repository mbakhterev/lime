(
	.FEnv (("L"; '44.1."="); .F
	(
		.Nth op (.FIn (); (1));
		.FPut (0;
			(("lvalue"; "left"); ("rvalue"; "right")); .F
		(
			.Nth l (.FIn (); (1; 0));
			.Nth r (.FIn (); (1; 1));

			.wr (.T ("I"; 4); l; r);

			// Адрес надо ещё оставить на тот случай, если выражение
			// будет использовано в качестве rvalue. Си - это такой
			// Си

			.FOut (0; (
				("address"; l)))
		));

